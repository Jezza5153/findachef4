
# Firebase Environment Variables (Template for your .env.local file)
# IMPORTANT: Copy these to a .env.local file and fill in your actual values.
# .env.local IS NOT committed to Git (ensure it's in .gitignore).

# For NEW Firebase project: "findachef2"
NEXT_PUBLIC_FIREBASE_API_KEY="YOUR_NEW_FINDACHEF2_API_KEY"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="findachef2.firebaseapp.com"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="findachef2"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="findachef2.appspot.com" # Or findachef2.firebasestorage.app - verify in Firebase console
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="YOUR_NEW_FINDACHEF2_MESSAGING_SENDER_ID"
NEXT_PUBLIC_FIREBASE_APP_ID="YOUR_NEW_FINDACHEF2_APP_ID"
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID="YOUR_NEW_FINDACHEF2_MEASUREMENT_ID"

# Genkit / Google AI API Key
GOOGLE_API_KEY="YOUR_GOOGLE_AI_API_KEY"

# Stripe API Keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_YOUR_STRIPE_PUBLISHABLE_KEY"
STRIPE_SECRET_KEY="sk_test_YOUR_STRIPE_SECRET_KEY"
STRIPE_WEBHOOK_SECRET="whsec_YOUR_STRIPE_WEBHOOK_SECRET"
